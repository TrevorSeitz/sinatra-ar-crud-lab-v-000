<h1>Update Post</h1>


<form action="/posts/:id" method="post">
<input id="hidden" type="hidden" name="_method" value="patch">
  <p>Title: </p><input type="text" name="name" value="<%= @post.name %>"><br>
  <p><%= @post.content %></p>
  <br>

<input type="submit" id="Submit" value="submit">

</form>
